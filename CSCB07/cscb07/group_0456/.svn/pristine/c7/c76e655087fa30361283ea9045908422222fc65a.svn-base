package system;

public class Node {
  
  private Document data;
  private Node left;
  private Node right;

  public Node(Document d) {
    data = d;
    left = null;
    right = null;
  }

  public Node getLeftNode() {
    return this.left;
  }

  public Node getRightNode() {
    return this.right;
  }

  public void setLeftNode(Node n) {
    left = n;
  }

  public void setRightNode(Node n) {
    right = n;
  }

  public Document getData() {
    return data;
  }

}
